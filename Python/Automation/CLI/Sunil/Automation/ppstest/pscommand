
## Monitoring "ps" command output 
rm old.txt
rm new.txt
rm diff.txt
# Initial "ps" output
ps | grep -v grep | grep TimApp >> old.txt
ps | grep -v grep | grep ipv6app >> old.txt
ps | grep -v grep | grep dsrcproxy >> old.txt
ps | grep -v grep | grep Monitord >> old.txt
sleep 30
# Current ps output
ps | grep -v grep | grep TimApp >> new.txt
ps | grep -v grep | grep ipv6app >> new.txt
ps | grep -v grep | grep dsrcproxy >> new.txt
ps | grep -v grep | grep Monitord >> new.txt
# Difference between old and current "ps" command output
diff -u old.txt new.txt > diff.txt
r=`wc -w diff.txt | awk '{print $1}'`
if [ $r -eq 0 ]
then
  var="match"
  echo "match"
else
  cat diff.txt
  var="not match"
  echo "not match"
fi    

