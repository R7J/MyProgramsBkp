### memory info
echo 3 > /proc/sys/vm/drop_caches
sync
echo "Memory information"
free

## CPU usage 
idle=`mpstat| grep all | awk '{print $10}'`
echo " "
echo -n "CPU Usage                                 :";echo "100-$idle" | bc 
## Nojournal usage
echo -n "Nojournal                                 :";df | grep nojournal | awk '{print $5}'
var="system is working"
